{
  "expo": {
    "scheme": "migaleria",
    "name": "Galería Multimedia",
    "slug": "Galería",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/galeria-logo.png",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/LogoMio.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.migaleria.app"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    }
  }
}